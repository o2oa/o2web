        "items": {
          "action_open": {
            "title": "open",
            "text": "open",
            "action": "action_open",
            "condition": "function( d ){ return true }",
            "styles": "documentActionNode",
            "mouseoverStyles": "documentActionNode_over",
            "mousedownStyles": "documentActionNode_down"
