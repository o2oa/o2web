        " <div item='progressAction' styles='documentSubject'></div>" +
        " <span  styles='documentIcon'></span>" +
        "</td>",
        "items": {
          "title": {},
          "progressAction":{"value" : "function(d){ return (d.progressAction && d.progressAction.length > 50) ? d.progressAction.substr(0,50)+'...' : d.progressAction || ''}"},
          "shortWorkDetail":{},
          "workType": {},
          "workLevel": {}
        }
