    "position" : "absolute",
    "left" : "0px",
    "height": "51px",
    "width": "0",
    "border-left" : "8px solid #ccc",
    "border-right" : "0px",
    "border-top": "4px solid transparent",
    "border-bottom":"4px solid transparent",
    "cursor" : "pointer"
  },
  "trapezoid_toLeft" : {
    "position" : "absolute",
    "left" : "0px",
    "height": "51px",
    "width": "0",
    "border-left" : "0px",
    "border-right" : "8px solid #ccc",
    "border-top": "4px solid transparent",
    "border-bottom":"4px solid transparent",
    "cursor" : "pointer"
