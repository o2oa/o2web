{
  "node": {
    "box-shadow": "inset 1px 0 0 0 #CCCCCC",
    "background": "#F8F8F8",
    "width": "300px",
    "height": "490px",
    "position": "absolute",
    "right": "0px",
    "top": "50px",
    "opacity": "0",
    "-webkit-user-select": "text",
    "-moz-user-select": "text"
  },
  "contentNode": {
    "overflow": "hidden"
  },
  "contentLine": {
    "height": "1px",
    "border-bottom": "1px dashed #ccc",
